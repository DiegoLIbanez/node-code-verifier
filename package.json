{
  "name": "code-verifier",
  "version": "1.0.0",
  "description": "Backend Node +  TS + Express + Postman",
  "main": "index.js",
  "scripts": {
    "swagger": "tsoa spec",
    "dev": "concurrently \"npx tsc --watch\" \"npm run swagger\" \"nodemon -q dist/index.js\" ",
